{
  "consumer": {
    "name": "Service-A-consumer"
  },
  "provider": {
    "name": "Service-C-messaging-provider"
  },
  "messages": [
    {
      "description": "an order message",
      "metaData": {
        "contentType": "application/json; charset\u003dUTF-8"
      },
      "contents": {
        "recipient": "John Doe",
        "id": "87f8fcd0-bdb8-4373-9eda-352c1d51eeab",
        "products": [
          1,
          2
        ]
      },
      "matchingRules": {
        "body": {
          "$.id": {
            "matchers": [
              {
                "match": "regex",
                "regex": "([a-f0-9]{8}(-[a-f0-9]{4}){4}[a-f0-9]{8})"
              }
            ],
            "combine": "AND"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.6.12"
    }
  }
}